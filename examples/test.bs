function addTwo (int& num) {
    num += 2;
}

function getMax (int a, int b) -> int {
    if (a >= b) {
        return a;
    }
    return b;
}

function conditionalCheck (int a) {
    if (a == 1) {
        print("1");
    }
    else if (a == 2) {
        print("2");
    }
    else {
        print("something");
    }
}

